<%= form_for current_user,:url => image_path, :action => :update, :html => {:multipart => true} do |f| %>
    <%= render 'shared/error_messages', object: f.object %>
    <div class="field">
      <%= f.file_field :image %>
      <%= f.hidden_field :element, :value => "user" %>
    </div>
    <%= f.submit "Change Picture", class: "btn btn-large btn-primary" %>
<% end %>